{
   "author": "Greg McLeod",
   "background": {
      "scripts": [ "app.js" ]
   },
   "browser_action": {
      "default_icon": "icons/icon_38.png",
      "default_title": "syncmarx (alpha)"
   },
   "description": "Web extension built for cross-browser bookmark syncing to the cloud.",
   "icons": {
      "48": "icons/icon_48.png",
      "96": "icons/icon_96.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "syncmarx",
   "permissions": [ "bookmarks", "https://*.dropboxapi.com/", "https://*.google.com/", "https://*.googleapis.com/", "https://*.box.com/", "storage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.7.0"
}
