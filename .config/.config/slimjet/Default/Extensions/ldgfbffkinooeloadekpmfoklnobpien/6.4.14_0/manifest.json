{
   "author": "Mussabekov Rustem",
   "background": {
      "persistent": false,
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": {
         "16": "assets/action_chrome_16.png",
         "24": "assets/action_chrome_24.png",
         "32": "assets/action_chrome_32.png"
      },
      "default_popup": "index.html?browser_action"
   },
   "commands": {
      "_execute_browser_action": {
         "suggested_key": {
            "chromeos": "Ctrl+Shift+E",
            "default": "Ctrl+Shift+E",
            "linux": "Ctrl+Shift+E",
            "mac": "Command+Shift+E",
            "windows": "Ctrl+Shift+E"
         }
      },
      "open_raindrop": {
         "description": "__MSG_openRaindrop__"
      },
      "save_page": {
         "description": "__MSG_savePage__",
         "suggested_key": {
            "chromeos": "Ctrl+Shift+S",
            "default": "Ctrl+Shift+S",
            "linux": "Ctrl+Shift+S",
            "mac": "Command+Shift+S",
            "windows": "Ctrl+Shift+S"
         }
      }
   },
   "content_security_policy": "script-src 'self' https://app.raindrop.io https://*.sentry.io https://sentry.io ; object-src 'none';",
   "default_locale": "en",
   "description": "__MSG_appDesc__",
   "homepage_url": "https://app.raindrop.io",
   "icons": {
      "128": "assets/icon_128.png",
      "16": "assets/icon_16.png",
      "32": "assets/icon_32.png",
      "48": "assets/icon_48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Raindrop.io",
   "omnibox": {
      "keyword": "rd"
   },
   "optional_permissions": [ "tabs" ],
   "permissions": [ "contextMenus", "activeTab", "storage" ],
   "short_name": "Raindrop.io",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "6.4.14",
   "version_name": "5.4.14",
   "web_accessible_resources": [ "index.html", "portal.html" ]
}
